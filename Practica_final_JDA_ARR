    "#creamos data y train y test\n",
    "\n",
    "dummies=pd.get_dummies(data2, columns=data2.columns, drop_first=True )\n",
    "train_df, test_df = train_test_split (dummies, test_size= 0.9, random_state=100)"
   ]
